{
  "name": "ise-fe-lint",
  "version": "1.4.0",
  "description": "ISE 前端编码规范工程化标准脚手架",
  "bin": "./lib/cli.js",
  "main": "./lib/index.js",
  "scripts": {
    "copyfiles": "copyfiles -a -u 1 \"src/config/**\" lib",
    "dev": "npm run copyfiles && tsc -w",
    "build": "rm -rf lib && npm run copyfiles && tsc",
    "test": "npm run build && jest",
    "coverage": "nyc jest --silent --forceExit",
    "prepublishOnly": "npm run test"
  },
  "keywords": [
    "ise",
    "lint",
    "cli"
  ],
  "files": [
    "lib/"
  ],
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >=16.0.0"
  },
  "author": "mj <boss_think@163.com>",
  "homepage": "https://github.com/ise-coder/ise-fe-spec#readme",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ise-coder/ise-fe-spec.git"
  },
  "bugs": {
    "url": "https://github.com/ise-coder/ise-fe-spec/issues"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "jest": "^27.5.1",
    "nyc": "^15.1.0",
    "typescript": "^4.6.2",
    "@types/eslint": "^8.4.1",
    "@types/fs-extra": "^9.0.13",
    "@types/prettier": "^2.4.4",
    "@types/stylelint": "^14.0.0",
    "@types/cross-spawn": "^6.0.2",
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.180",
    "@types/ejs": "^3.1.0"
  },
  "dependencies": {
    "eslint": "^8.7.0",
    "stylelint": "^14.3.0",
    "markdownlint": "^0.25.1",
    "ora": "^5.1.0",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.3",
    "cross-spawn": "^7.0.3",
    "chalk": "^4.1.0",
    "command-exists": "^1.2.9",
    "glob": "^7.1.6",
    "lodash": "^4.17.20",
    "ejs": "^3.1.5",
    "commander": "^8.0.0"
  }
